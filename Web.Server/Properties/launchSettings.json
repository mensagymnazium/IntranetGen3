{
	"useWebAssemblyDebugging": true,
	"iisSettings": {
		"windowsAuthentication": false,
		"anonymousAuthentication": true,
		"iisExpress": {
			"applicationUrl": "https://localhost:44347/",
			"sslPort": 44347
		}
	},
	"profiles": {
		"IIS Express": {
			"commandName": "IISExpress",
			"launchBrowser": true,
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}"
		},
		"Web.Server": {
			"commandName": "Project",
			"launchBrowser": true,
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"applicationUrl": "https://localhost:44319;http://localhost:9979",
			"inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}"
		},
		"WSL": {
			"commandName": "WSL2",
			"launchBrowser": true,
			"launchUrl": "https://localhost:44319",
			"environmentVariables": {
				"ASPNETCORE_URLS": "https://localhost:44319;http://localhost:9979",
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"distributionName": ""
		}
	}
}